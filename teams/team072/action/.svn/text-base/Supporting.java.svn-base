package team072.action;

import battlecode.common.GameActionException;
import battlecode.common.MapLocation;
import battlecode.common.RobotController;
import battlecode.common.RobotLevel;

/**
 * Transfer energon to nearby unit
 */
public class Supporting {

	/**
	 * The first parameter should be the maplocation of the nearby wout. The
	 * second parameter should be the amount of energon it should give to the
	 * nearby wout.
	 */
	public static void act(RobotController myRC, MapLocation loc, Double blood,
			RobotLevel level) throws GameActionException {
		myRC.transferUnitEnergon(blood, loc, level);
		// TODO Auto-generated method stub

	}

}
